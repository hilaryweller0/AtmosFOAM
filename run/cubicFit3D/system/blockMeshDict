FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

convertToMeters 1000; // effective units are km

vertices (
    (-150.5 -150.5 0)  // 0
    (150.5  -150.5 0)  // 1
    (150.5  150.5 0)  // 2
    (-150.5 150.5 0)  // 3
    (-150.5 -150.5 15) // 4
    (150.5  -150.5 15) // 5
    (150.5  150.5 15) // 6
    (-150.5 150.5 15) // 7
);

blocks (
	hex (0 1 2 3 4 5 6 7)
	    (26 26 15)
	    simpleGrading (1 1 1)
);

boundary (
	inlet {
		type patch;
		faces (
			(0 3 7 4)
		);
	}

	outlet {
		type patch;
		faces (
			(1 2 6 5)
		);
	}

	ground {
		type wall;
		faces (
			(0 1 2 3)
		);	
	}

	top {
		type wall;
		faces (
			(4 5 6 7)
		);
	}

	front {
		type wall;
		faces (
			(0 1 5 4)
		);
	}

	back {
		type wall;
		faces (
			(3 2 6 7)
		);
	}
);
