FoamFile
{
    version         1.4;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XY;

FieldsToPlot
(
    [h filledContours 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [stable.U vectors 1e4 5 "black"]
    [buoyant.U vectors 1e4 5 "grey"]
);

plotBoundaryDots false;
annotate "minMax 0 -38000";

