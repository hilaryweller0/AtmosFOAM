FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application buoyantFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         10e5;

deltaT          500; //1e3;

writeControl    runTime;

writeInterval   1e5;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

adjustTimeStep no;
maxCo          1;

libs            ("libshallowWater.so");

