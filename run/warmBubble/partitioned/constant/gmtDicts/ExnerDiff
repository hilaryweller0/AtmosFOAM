FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [ExnerDiff filledContours 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [UfDiff vectors 0 17 "black"]
);

//separateLegend true;
//region "-5000/5000/0/1.25e4";
annotate "minMax -1.2e3 400";
boundaryMarks "2500/2500";
plotBoundaryDots false;
