FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;

gradSchemes
{
    default         none;
    grad(Phi)       leastSquares; //Gauss linear; //pointLinear;//
    grad(gradPhi)   Gauss linear; //leastSquares; //pointLinear; //Gauss linear;
}

divSchemes
{
    default             none;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default        none;
}

snGradSchemes
{
    default         none;
    snGrad(((domainIntegrate(detHess)|domainIntegrate((1|monitorNew)))|monitorR)) uncorrected;
    snGrad(c_mR) uncorrected;
}

fluxRequired
{
    default         no;
}


