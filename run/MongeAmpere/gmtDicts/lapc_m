FoamFile
{
    version         2.2;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [lapc_m filledContours 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [gradc_m vectors 1e-3 1 "black"]
    [none mesh "grey"]
);

annotate "minMax 0.125 -0.003";
region "0/0.5/0/0.5";

