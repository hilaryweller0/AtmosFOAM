// dummy file to placate paraView
FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

offCentre 0.5;
SIgravityWaves false;

interpolationSchemes {
    default         none;
    interpolate(theta) linear;
    interpolate(rho) linear;
    interpolate(convection(U,u)) linear;
    H linear;
    interpolate(u) linear;
    interpolate(grad(thetaf)) linear;
    thetaFromb linear;
}

divSchemes {
    default         none;
//    div(U,u)        Gauss cubicUpwindCPCFit false;
//    div(U,theta)    Gauss cubicUpwindCPCFit false;
    div(U,u)        Gauss linearUpwind grad(u);
    div(U,theta)    Gauss linearUpwind grad(theta);
}

gradSchemes {
    default         none;
    grad(theta)     Gauss linear;
    grad(Exner)     Gauss linear;
    grad(u)         Gauss linear;
    grad(theta)     Gauss linear;
}

laplacianSchemes {
    default         Gauss linear uncorrected;
    laplacian(gradPcoeff,Exner) Gauss linear uncorrected;
    laplacian((0.5*(((((G*0.5)*Cp)*deltaT)*rhof)*thetaf)),Exner) Gauss linear uncorrected;
}

ddtSchemes {
    default         none;
    ddt(rho)        Euler;
    ddt(rho,theta)  Euler;
    ddt(Psi,Exner)  Euler;
}

fluxRequired
{
    default         no;
    Exner;
}
