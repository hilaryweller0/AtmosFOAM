FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1000;

vertices
(
    (-150 0 0)
    (150 0 0)
    (150 1 0)
    (-150 1 0)
    (-150 0 25)
    (150 0 25)
    (150 1 25)
    (-150 1 25)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (300 1 50) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    top
    {
        type  wall;
        faces ((4 5 6 7));
    }
    inlet
    {
        type            cyclic;
        neighbourPatch  outlet;
        faces           ((0 4 7 3));
    }
    outlet
    {
        type            cyclic;
        neighbourPatch  inlet;
        faces           ((2 6 5 1));
    }
    ground
    {
        type wall;
        faces ((0 3 2 1));
    }
    front
    {
        type empty;
        faces ((0 1 5 4));
    }
    back
    {
        type empty;
        faces ((2 3 7 6));
    }
);

mergePatchPairs
(
);

